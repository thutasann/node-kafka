{
	"compilerOptions": {
		"target": "es2016",
		"module": "CommonJS",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": false,
		"strict": true,
		"skipLibCheck": true,
		"noImplicitAny": false,
		"strictPropertyInitialization": false,
		"experimentalDecorators": true,
		"rootDir": "./src",
		"outDir": "./dist"
	},
	"exclude": ["jest.config.ts"]
}
